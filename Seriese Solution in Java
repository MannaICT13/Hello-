import java.util.Scanner;
public class Main {

public static void main(String[] args) {
		// TODO Auto-generated method stub

	Scanner sc=new Scanner(System.in);
	
	int T=sc.nextInt();
	
	for(int i=1;i<=T;i++){
		
		int N=sc.nextInt();
		
		int x=(int) Math.pow(((N*(N+1))/2),2);
		
		System.out.println("Case "+i+":"+" "+x);
		
	
	
	}
	
	}

}
